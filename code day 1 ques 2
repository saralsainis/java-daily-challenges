    String go="Gold";
        String si="Silver";
      
        Double g=10.0;
  
        
       
        
     
      
    String s=obj.next();
        
     Double c= obj.nextDouble();
    Double l=(b/g)*c;
        Double ca=(a/1000)*c;
        
        if (c<0.01 || c>1000){
            System.out.println("Invalid Input");
        }
 else if (s.equals(go)){
            
            System.out.println(l.intValue());
            
            
        }
        else if (s.equals(si)){
            
            System.out.println(ca.intValue());
       
        }
        

 
